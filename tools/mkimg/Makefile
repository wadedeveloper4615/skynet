WORKDIR = %cd%

CC = gcc.exe
CXX = g++.exe
AR = ar.exe
LD = g++.exe
WINDRES = windres.exe

INC = 
CFLAGS =  -w
RESINC = 
LIBDIR = 
LIB = 
LDFLAGS = 

INC_DEBUG =  $(INC)
CFLAGS_DEBUG =  $(CFLAGS) -w -std=c++11 -std=c11 -m32 -g
RESINC_DEBUG =  $(RESINC)
RCFLAGS_DEBUG =  $(RCFLAGS)
LIBDIR_DEBUG =  $(LIBDIR)
LIB_DEBUG = $(LIB)
LDFLAGS_DEBUG =  $(LDFLAGS) -static-libstdc++ -static-libgcc -static -m32
OBJDIR_DEBUG = obj\\Debug
DEP_DEBUG = 
OUT_DEBUG = bin\\Debug\\mkimg.exe

INC_RELEASE =  $(INC)
CFLAGS_RELEASE =  $(CFLAGS) -O3 -w -std=c++11 -std=c11 -m32
RESINC_RELEASE =  $(RESINC)
RCFLAGS_RELEASE =  $(RCFLAGS)
LIBDIR_RELEASE =  $(LIBDIR)
LIB_RELEASE = $(LIB)
LDFLAGS_RELEASE =  $(LDFLAGS) -s -static-libstdc++ -static-libgcc -static -m32
OBJDIR_RELEASE = obj\\Release
DEP_RELEASE = 
OUT_RELEASE = bin\\Release\\mkimg.exe

OBJ_DEBUG = $(OBJDIR_DEBUG)\\src\\file.o $(OBJDIR_DEBUG)\\src\\mkdosfs.o

OBJ_RELEASE = $(OBJDIR_RELEASE)\\src\\file.o $(OBJDIR_RELEASE)\\src\\mkdosfs.o

all: debug release

clean: clean_debug clean_release

before_debug: 
	cmd /c if not exist bin\\Debug md bin\\Debug
	cmd /c if not exist $(OBJDIR_DEBUG)\\src md $(OBJDIR_DEBUG)\\src

after_debug: 

debug: before_debug out_debug after_debug

out_debug: before_debug $(OBJ_DEBUG) $(DEP_DEBUG)
	$(LD) $(LIBDIR_DEBUG) -o $(OUT_DEBUG) $(OBJ_DEBUG)  $(LDFLAGS_DEBUG) $(LIB_DEBUG)

$(OBJDIR_DEBUG)\\src\\file.o: src\\file.cpp
	$(CXX) $(CFLAGS_DEBUG) $(INC_DEBUG) -c src\\file.cpp -o $(OBJDIR_DEBUG)\\src\\file.o

$(OBJDIR_DEBUG)\\src\\mkdosfs.o: src\\mkdosfs.cpp
	$(CXX) $(CFLAGS_DEBUG) $(INC_DEBUG) -c src\\mkdosfs.cpp -o $(OBJDIR_DEBUG)\\src\\mkdosfs.o

clean_debug: 
	cmd /c del /f $(OBJ_DEBUG) $(OUT_DEBUG)
	cmd /c rd bin\\Debug
	cmd /c rd $(OBJDIR_DEBUG)\\src

before_release: 
	cmd /c if not exist bin\\Release md bin\\Release
	cmd /c if not exist $(OBJDIR_RELEASE)\\src md $(OBJDIR_RELEASE)\\src

after_release: 

release: before_release out_release after_release

out_release: before_release $(OBJ_RELEASE) $(DEP_RELEASE)
	$(LD) $(LIBDIR_RELEASE) -o $(OUT_RELEASE) $(OBJ_RELEASE)  $(LDFLAGS_RELEASE) $(LIB_RELEASE)

$(OBJDIR_RELEASE)\\src\\file.o: src\\file.cpp
	$(CXX) $(CFLAGS_RELEASE) $(INC_RELEASE) -c src\\file.cpp -o $(OBJDIR_RELEASE)\\src\\file.o

$(OBJDIR_RELEASE)\\src\\mkdosfs.o: src\\mkdosfs.cpp
	$(CXX) $(CFLAGS_RELEASE) $(INC_RELEASE) -c src\\mkdosfs.cpp -o $(OBJDIR_RELEASE)\\src\\mkdosfs.o

clean_release: 
	cmd /c del /f $(OBJ_RELEASE) $(OUT_RELEASE)
	cmd /c rd bin\\Release
	cmd /c rd $(OBJDIR_RELEASE)\\src

virtual_all: debug release

.PHONY: before_debug after_debug clean_debug before_release after_release clean_release

